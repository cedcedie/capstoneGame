        <mxCell id="XKVN2B2UxLaelDESN50H-28" value="SCENE 2&lt;br&gt;LEO&#39;S WAKE" style="rounded=0;whiteSpace=wrap;html=1;" vertex="1" parent="WIyWlLk6GJQsqaUBKTNV-1">
